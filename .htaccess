Options -Indexes
RewriteEngine on

RewriteCond %{HTTP_HOST} ^pocchong\.de$ [NC]
RewriteRule ^(.*)$ http://www.pocchong.de/$1 [R=301,L,NC]

RewriteRule ^a/([^/]+)\.cgi /cgi-bin/admin/$1.cgi [L,NC]
RewriteRule ^a/?$ /cgi-bin/admin/admin.cgi [L,NC]

RewriteRule ^p/(\w.+?)(\.html)?$ /html/page/_indiv/$1.php [L,NC]

RewriteRule ^about/?$ /html/page/_indiv/about.php [L,NC]
RewriteRule ^maxi/?$ /html/page/_indiv/maxi.php [L,NC]
RewriteRule ^gamelist/?$ /html/page/_indiv/gamelist.php [L,NC]

RewriteRule ^exec/(.+)/?$ /html/page/exec/$1 [L,NC]

RewriteRule ^archiv/post/(20\d{2})/?$ /cgi-bin/main/archiv.cgi?sel=post&amp;year=$1 [L,NC]
RewriteRule ^archiv/([a-z]+)/offset/(\d+)/?$ /cgi-bin/main/archiv.cgi?sel=$1&amp;offset=$2 [L,NC]
RewriteRule ^archiv/([a-z]+)/?$ /cgi-bin/main/archiv.cgi?sel=$1 [L,NC]
RewriteRule ^archiv/?$ /cgi-bin/main/archiv.cgi [L,NC]

RewriteRule ^(mygirls|post)/(\d+)/?$ /cgi-bin/main/$1.cgi?id=$2 [L,NC]
RewriteRule ^(mygirls|post)/?$ /cgi-bin/main/$1.cgi [L,NC]

RewriteRule ^cyouwa/?$ /html/page/cyouwa/index.php [L,NC]
RewriteRule ^cyouwa/(.+)\.html$ /html/page/cyouwa/$1.php [L,NC]
RewriteRule ^cyouwa/(.+)/?$ /html/page/cyouwa/$1 [L,NC]
